<html>
  <head>
    <%= stylesheet_link_tag 'application' %>
  </head>
  <body>
    <%= yield %>
    <div class="navbar navbar-inverse navbar-fixed-top" role="navigation">
      <div class="container">
        <%= link_to "gCamp", homepage_path, class: "navbar-brand"  %>
      </div>
    </div>

    <div class="footer navbar-fixed-bottom">
      <div class="container">
        <%= link_to :Home, homepage_path, {:class => ("active disabled" if current_page?('/homepage'))}%> |
        <%= link_to :About, about_path, {:class => ("active disabled" if current_page?('/about'))}%> |
        <%= link_to :Terms, terms_path, {:class => ("active disabled" if current_page?('/terms'))}%> |
        <%= link_to :FAQ, faq_path, {:class => ("active disabled" if current_page?('/faq'))}%>
      </div>
    </div>
  </body>
</html>
